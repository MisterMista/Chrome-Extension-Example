{
    "manifest_version": 2,
    "name": "Test Extension",
    "version": "0.1",
    "description": "A test extension for learning purposes.",
    "icons": {
        "32": "icon.png"
    },

    "browser_action": {
        "default_icon": "icon.png"
    },

    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["jquery-3.3.1.min.js","content.js"]
        }
    ],

    "background": {
        "scripts": ["background.js"]
    }
}
